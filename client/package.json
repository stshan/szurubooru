    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babelify": "^8.0.0",
    "browserify": "^16.2.2",
    "csso": "^3.5.1",
    "glob": "^7.1.2",
    "html-minifier": "^3.5.18",
    "jimp": "^0.13.0",
